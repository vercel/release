{
  "name": "release",
  "version": "1.1.3",
  "description": "Manage GitHub Releases from the command line",
  "scripts": {
    "coverage": "nyc --reporter=lcov --reporter=text ava",
    "lint": "xo",
    "test": "ava && xo",
    "test:unit": "ava"
  },
  "ava": {
    "require": [
      "async-to-gen/register"
    ]
  },
  "files": [
    "bin",
    "lib"
  ],
  "repository": "zeit/release",
  "keywords": [
    "github",
    "releases",
    "cli",
    "tags",
    "push"
  ],
  "author": "leo",
  "license": "MIT",
  "bin": {
    "release": "./bin/release.js"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "import/no-dynamic-require": 0,
      "import/no-unresolved": 0,
      "import/no-unassigned-import": 0,
      "unicorn/no-process-exit": 0,
      "camelcase": 0
    }
  },
  "engines": {
    "node": ">= 7.0.0"
  },
  "bugs": {
    "url": "https://github.com/zeit/release/issues"
  },
  "homepage": "https://github.com/zeit/release#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "nyc": "^10.0.0",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "args": "^2.2.3",
    "async-retry": "^0.2.1",
    "async-to-gen": "^1.3.0",
    "capitalize": "^1.0.0",
    "chalk": "^1.1.3",
    "configstore": "^2.1.0",
    "git-commits": "^1.2.0",
    "git-repo-name": "^0.6.0",
    "git-state": "^3.1.0",
    "git-username": "^0.5.0",
    "github": "^8.0.0",
    "inquirer": "^3.0.0",
    "node-version": "^1.0.0",
    "open": "0.0.5",
    "ora": "^1.0.0",
    "pluralize": "^3.1.0",
    "random-string": "^0.1.2",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3",
    "semver": "^5.3.0",
    "tagged-versions": "^1.3.0",
    "then-sleep": "^1.0.1",
    "trim": "0.0.1",
    "update-notifier": "^1.0.3"
  }
}
